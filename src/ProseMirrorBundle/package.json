{
  "name": "silentnotes-prosemirror",
  "version": "1.0.0",
  "description": "Transpiles and bundles the required JS files of ProseMirror for SilentNotes",
  "repository": {
    "type": "git",
    "url": "https://github.com/martinstoeckli/SilentNotes"
  },
  "scripts": {
    "build": "esbuild src/prose-mirror-bundle.ts --bundle --minify --tsconfig-raw=true --format=esm --keep-names --tree-shaking=true --target=es2020,chrome67,edge18,firefox60,node12,safari11 --outfile=../SilentNotes.Blazor/wwwroot/prose-mirror-bundle.js",
    "debug": "esbuild src/prose-mirror-bundle.ts --bundle --tsconfig-raw=true --format=esm --keep-names --tree-shaking=true --target=es2020,chrome67,edge18,firefox60,node12,safari11 --outfile=../SilentNotes.Blazor/wwwroot/prose-mirror-bundle.js"
  },
  "keywords": [],
  "author": "Martin Stoeckli",
  "license": "MPL-2.0",
  "devDependencies": {
    "core-js": "^3.38.1",
    "typescript": "latest"
  },
  "dependencies": {
    "@tiptap/core": "^3.3.0",
    "@tiptap/extension-blockquote": "^3.3.0",
    "@tiptap/extension-bold": "^3.3.0",
    "@tiptap/extension-code": "^3.3.0",
    "@tiptap/extension-code-block": "^3.3.0",
    "@tiptap/extension-code-block-lowlight": "^3.3.0",
    "@tiptap/extension-document": "^3.3.0",
    "@tiptap/extension-hard-break": "^3.3.0",
    "@tiptap/extension-heading": "^3.3.0",
    "@tiptap/extension-history": "^3.3.0",
    "@tiptap/extension-italic": "^3.3.0",
    "@tiptap/extension-link": "^3.3.0",
    "@tiptap/extension-list": "^3.3.0",
    "@tiptap/extension-paragraph": "^3.3.0",
    "@tiptap/extension-strike": "^3.3.0",
    "@tiptap/extension-text": "^3.3.0",
    "@tiptap/extension-text-style": "^3.3.0",
    "@tiptap/extension-underline": "^3.3.0",
    "esbuild": "^0.25.9",
    "prosemirror-commands": "^1.6.0",
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-model": "^1.23.0",
    "prosemirror-schema-list": "^1.4.1",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.10.1",
    "prosemirror-view": "^1.34.3"
  }
}
